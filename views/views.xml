                      <field name="select_2_2" widget="many2many_tags"/>
                    </group>
                  </page>              
                  <!--Daño vástago-->
                  <page string="Vista daño vástago" name="daño_vastago">                  
                    <group name="daño_vastago" string="Vista daño vástago">
                      <field name="img_vastago_daño"/>
                      <field name="img_vastago_daño_2"/>
                      <field name="img_vastago_daño_3"/>
                      <field name="img_vastago_daño_4"/>
                      <field name="img_vastago_daño_5"/>
                      <field name="txt_vastago_daño"/>
                    </group>
                  </page>              


                  <page string="Vista general de anclaje de vástago" name="vista_general_anclaje_vastago">                                                      
